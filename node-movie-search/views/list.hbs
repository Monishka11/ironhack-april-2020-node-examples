<h2 class="page-title">Search Results</h2>

<ul>
  {{#each results}}
    <li class="movie-item">
      <a href="/{{ imdbID }}">
        <img src="{{ Poster }}" alt="{{ Title }}"/>
        <div class="movie-details">
          <strong>{{ Title }}</strong>
          <br>
          <small>{{ Year }}</small>
        </div>
      </a>
    </li>
  {{/each}}
</ul>
